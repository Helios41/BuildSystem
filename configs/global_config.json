{
   "architectures": 
   [
      "X86_64",
      "X86"
   ],
   "C":
   {
      "executable": 
      {
         "ending": ".exe",
         "commands":
         [
            "[ARCH: X86] \"C:/Program Files (x86)/Microsoft Visual Studio 12.0/VC/vcvarsall\" x86",
            "[ARCH: X86_64] \"C:/Program Files (x86)/Microsoft Visual Studio 12.0/VC/vcvarsall\" x86_amd64",
            "cd [BUILD_DIRECTORY]",
            "[OPT] cl /O2 /Fe[PROJECT_NAME] *.c /link *.lib [STATIC_LIBRARIES]",
            "[NOPT] cl /Od /Fe[PROJECT_NAME] *.c /link *.lib [STATIC_LIBRARIES]"
         ]
      },
      "dynamic": 
      {
         "ending": ".dll",
         "commands":
         [
            "vcvars32",
            "cd [BUILD_DIRECTORY]",
            "cl /LD /Fe[PROJECT_NAME] *.c /link *.lib [STATIC_LIBRARIES]"
         ]
      },
      "static": 
      {
         "ending": ".lib",
         "commands":
         [
            "vcvars32",
            "cd [BUILD_DIRECTORY]",
            "cl /c *.c",
            "lib /out:[PROJECT_NAME].lib *.obj"
         ]
      }
   },
   "D":
   {
      "executable":
      {
         "ending": ".exe",
         "commands":
         [
            "dub"
         ]
      }
   }
}