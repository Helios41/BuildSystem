{
   "C":
   {
      "executable": 
      {
         "ending": ".exe",
         "commands":
         [
            "vcvars32",
            "cd [BUILD_DIRECTORY]",
            "cl /Fe[PROJECT_NAME] *.c"
         ]
      },
      "dynamic": 
      {
         "ending": ".dll",
         "commands":
         [
            "vcvars32",
            "cd [BUILD_DIRECTORY]",
            "cl /Fe[PROJECT_NAME] *.c"
         ]
      },
      "static": 
      {
         "ending": ".lib",
         "commands":
         [
            "vcvars32",
            "cd [BUILD_DIRECTORY]",
            "cl /c *.c",
            "lib /out:[PROJECT_NAME].lib *.obj"
         ]
      }
   },
   "D":
   {
      "executable":
      {
         "ending": ".exe",
         "commands":
         [
            "dub"
         ]
      }
   }
}