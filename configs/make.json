{
   "default": "Func1",
   "Func1": 
   {
      "language": "C",
      "operations":
      [
         ["copy", "../dub.json", "../bob.json"],
         ["||", "../", "../test/", ".random"],
         ["delete", "../toremove.txt"],
         ["||", "../bob.json"],
         ["print", "before building!"],
         "build",
         ["print", "just built!"],
         ["print", "We're on an OS! ", "[OS: Windows] We're on Windows!"],
         ["[OS: Windows] print", "Printed on Windows"],
         ["[OS: Linux] print", "Printed on Linux"]
      ],
      "per-operations":
      [
         ["copy", "../dub.json", "[OUTPUT_DIRECTORY]/some_stuff/bob.json"],
         ["||", "../", "[OUTPUT_DIRECTORY]/jsons/", ".json"]
      ],
      "type": "executable",
      "build" : "../test/",
      "source": 
      [
         "../helloworld.c",
         ["../", ".h"]
      ],
      "attributes": 
      [
         "dependencies"
      ],
      "dependencies": 
      [
         "[OS: Windows] User32.lib",
         "[OS: Fake_Linux] SomeLib.so"
      ],
      "optimized": false,
      "project": "Test",
      "version": [0, 0, 0, "test"]
   },
   "MoveBob":
   {
      "operations":
      [
         ["move", "./bob.json", "./test/bob.json"]
      ]
   },
   "CallTest":
   {
      "operations":
      [
         ["call", "./configs/test.json", "Func1", "Func2"],
         ["||", "./configs/test.json"]
      ]
   }
}