{
   "default": "Func1",
   "Func1": 
   {
      "language": "C",
      "operations":
      [
         ["call", "./make_dl.json"],
         ["||", "./make_lib.json"],
         ["copy", "../dub.json", "../bob.json"],
         ["||", "../", "../test/", ".random"],         
         ["delete", "../toremove.txt"],
         ["||", "../bob.json"],
         ["print", "before building!"],
         "build",
         ["print", "just built!"],
         ["||", "DLTest Exports: [FIELD_REF: exports<-make_dl.json]"],
         ["print", "We're on an OS! ", {"if": "OS=Windows", "then": "We're on Windows!"}],
         {
            "if": "OS=Windows",
            "then": ["print", "Printed on windows"]
         },
         {
            "if": "OS=Linux",
            "then": ["print", "Printed on Linux"]
         },
         ["call", "=", "SayHi"]
      ],
      "per-operations":
      [
         ["copy", "../dub.json", "[OUTPUT_DIRECTORY]/some_stuff/bob.json"],
         ["||", "../", "[OUTPUT_DIRECTORY]/jsons/", ".json", "1"],
         ["||", "../", "[OUTPUT_DIRECTORY]/djson/", "d", ".json", "1"],
         ["||", "../dltest/[OS_NAME]_[ARCH_NAME]/DLTest.dll", "[OUTPUT_DIRECTORY]/DLTest.dll"],
         "build"
      ],
      "type": "executable",
      "build" : "../test/",
      "source": 
      [
         "../helloworld.c",
         ["../", ".h"]
      ],
      "dependencies": 
      [
         {
            "if": "OS=Windows",
            "then": "User32.lib"
         },
         {
            "if": "OS=FakeLinux",
            "then": "SomeLib.so"
         },
         ["../static_lib/[OS_NAME]_[ARCH_NAME]/", "static_lib", ".lib"]
      ],
      "optimized": false,
      "project": "Test",
      "version": [0, 0, 0, "test"],
      "version_break": "Ll"
   },
   "MoveBob":
   {
      "operations":
      [
         ["move", "../bob.json", "../test/bob.json"]
      ]
   },
   "SayHi":
   {
      "operations":
      [
         ["print", "hello"]
      ]
   },
   "CallTest":
   {
      "operations":
      [
         ["call", "test.json", "Func1", "Func2"],
         ["||", "test.json"]
      ]
   }
}